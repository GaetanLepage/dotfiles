# SSH tunneling to perception_workstation_alias through bastion.inrialpes.fr
Host alya
    User galepage
    HostName alya.inrialpes.fr
    ProxyCommand ssh -W %h:%p galepage@bastion.inrialpes.fr zsh # SSH tunneling to access1-cp through bastion.inrialpes.fr
    RemoteCommand zsh
    RequestTTY force

