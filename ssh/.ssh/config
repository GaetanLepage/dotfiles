# SSH tunneling to perception_workstation_alias through bastion.inrialpes.fr
Host alya
    User galepage
    HostName alya.inrialpes.fr
    ProxyCommand ssh -W %h:%p galepage@bastion.inrialpes.fr # SSH tunneling to access1-cp through bastion.inrialpes.fr

